{
  "extends": ["prettier", "eslint-config-umi"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true,
    "jest": true,
    "jasmine": true
  },
  "rules": {
    "no-console": 1,
    "no-const-assign": 2,
    "no-else-return": 2,
    "no-empty": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": 2,
    "no-extra-semi": 2,
    "no-fallthrough": 1,
    "no-invalid-regexp": 2,
    "no-mixed-spaces-and-tabs": [2, false],
    "no-multi-spaces": 1,
    "no-multi-str": 2,
    "no-multiple-empty-lines": [1, { "max": 2 }],
    "no-redeclare": 2,
    "no-spaced-func": 2,
    "no-trailing-spaces": 1,
    "no-this-before-super": 0,
    "no-undef": 1,
    "no-unreachable": 2,
    "no-unused-vars": [2, { "vars": "all", "args": "after-used" }],
    "no-use-before-define": 2,
    "no-var": 2,
    "comma-spacing": 0,
    "default-case": 2,
    "eqeqeq": 2,
    "quotes": [1, "single"]
  }
}
